    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arthurquinellato.igrocery"
    },
    "android": {
      "package": "com.arthurquinellato.igrocery",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app/light_app_logo.png",
        "backgroundColor": "#ffffff"
      },
      "dark": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/app/dark_app_logo.png",
          "backgroundColor": "#000000"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
