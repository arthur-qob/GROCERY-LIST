{
  "expo": {
    "name": "igrocery",
    "slug": "igrocery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app/light_app_logo.png",
    "scheme": "igrocery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arthurquinellato.igrocery"
    },
    "android": {
      "package": "com.arthurquinellato.igrocery",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app/light_app_logo.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "transparent",
          "dark": {
            "image": "./assets/images/app/dark_app_logo.png",
            "backgroundColor": "transparent",
            "resizeMode": "cover"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
